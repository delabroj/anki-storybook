{
  "printWidth": 120,
  "parser": "flow",
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.json",
      "options": { "parser": "json", "tabWidth": 2 }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "options": { "parser": "typescript" }
    }
  ]
}
